<div class="form">
<%= form_for(@restaurant) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :category %>
  <%= f.text_field :category %>
  <%= f.label :phone_number %>
  <%= f.number_field :phone_number %>
  <%= f.submit%>
  </div>
  <%= link_to "Back to home page", restaurants_path, class: "btn btn-primary"%>
<% end %>
